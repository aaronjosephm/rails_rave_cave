<div class="container full-height-list">
<div class="row">
  <div class="col-md-4 offset-md-4">
    <h1>Create a new warehouse</h1>

      <%= simple_form_for(@warehouse) do |f| %>
         <%= f.input :name %>
         <%= f.input :photo %>
         <%= f.input :photo_cache, as: :hidden %>
         <%= f.input :description %>
         <%= f.input :subtitle %>
         <%= f.input :address %>
         <%= f.input :price %>
         <%= f.submit "submit", class: "btn btn-primary" %>
       <% end %>

   </div>
 </div>
</div>
